{
	"name": "pangolicons",
	"version": "3.2.1",
	"description": "Pangolicons is a cute open source svg icon library inspired by feather icons.",
	"main": "./dist/pangolicons.js",
	"module": "./dist/pangolicons.esm.mjs",
	"types": "./dist/types/pangolicons.d.ts",
	"exports": {
		"require": "./dist/pangolicons.js",
		"import": "./dist/pangolicons.esm.mjs"
	},
	"scripts": {
		"dev": "rollup --config rollup.config.js --watch",
		"build": "rollup --config rollup.config.js",
		"prepublishOnly": "npm run build && npx tsc"
	},
	"keywords": [
		"svg",
		"icon",
		"glyph",
		"minimal",
		"dependency-free"
	],
	"sideEffects": false,
	"unpkg": "./dist/pangolicons.browser.min.js",
	"author": "Sebastian Heinz",
	"license": "MIT",
	"devDependencies": {
		"rollup": "^2.60.2",
		"rollup-plugin-terser": "^7.0.2",
		"typescript": "^4.5.2"
	}
}
